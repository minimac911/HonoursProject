@model WebMVC.Models.TenantManager.TenantCustomization

<tr>
    <td><a asp-area="" asp-controller="Admin" asp-action="ViewCustomization" asp-route-id="@Model.Id">@Model.Title</a></td>
    <td>@Model.ServiceName</td>
    <td>@Model.ControllerName</td>
    <td>@Model.MethodName</td>
    <td><span class="badge @(Model.IsActive ? "badge-success" : "badge-danger")">@(Model.IsActive ? "Active" : "Inactive")</span></td>
</tr>